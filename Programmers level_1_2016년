// 2016ë…„ https://programmers.co.kr/learn/courses/30/lessons/12901
#include <iostream>
using namespace std;

string solution(int a, int b) {
	string answer = "";

	int month[12] = { 0,31,29,31,30,31,30,31,31,30,31,30 };
	int day = 0;

	for (int i = 0; i < a; i++)
	{
		day += month[i];
	}
	day += b;

	if (day % 7 == 0)
		answer = "THU";
	else if (day % 7 == 1)
		answer = "FRI";
	else if (day % 7 == 2)
		answer = "SAT";
	else if (day % 7 == 3)
		answer = "SUN";
	else if (day % 7 == 4)
		answer = "MON";
	else if (day % 7 == 5)
		answer = "TUE";
	else if (day % 7 == 6)
		answer = "WED";

	return answer;
}
